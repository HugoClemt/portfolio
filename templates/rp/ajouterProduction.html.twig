{% extends "entete.html.twig" %}
{% block content %}
<body>
<div class="container">
    <center><h1>Ajouter une production</h1></center>
       <br> 
       <div class="form-group col-md-12">
			{{ form_start(form) }}
            <div>
                <label>Description</label> 
                {{ form_widget(form.designation, {'attr': {'class': 'form-control'}})}}  
            </div><br>

            <div class="form-group col-md-12">
				<label>URL</label>
				{{ form_widget(form.url, {'attr': {'class': 'form-control'}})}}
			</div>
              <center><div class="col-12">
					{{ form_widget(form.enregistrer, {'attr': {'class': 'btn btn-secondary'}})}}
				</div></center>
			</div>           
</div>
</body>
{% endblock %}

